<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App2.ProfileEditorPage">
    <ContentPage.Content>
        <StackLayout Padding="20">
            <Entry x:Name="FirstNameEntry" Placeholder="First Name" />
            <Entry x:Name="SecondNameEntry" Placeholder="Second Name" />
            <Entry x:Name="PatronymicEntry" Placeholder="Patronymic" />
            <Entry x:Name="Opis" Placeholder="Opisanie" />
            <Entry x:Name="Skils" Placeholder="Skills" />
            <Button Text="Save" Clicked="SaveButton_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>